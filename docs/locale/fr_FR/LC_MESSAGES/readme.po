msgstr "voisorth : nombre de voisins orthographiques"

#: ../../../README.rst:140 8fbaedc9b9654134852143b0b5b223f5
msgid "voisphon: number of phonological neighbors"
msgstr "voisphon : nombre de voisins phonologiques"

#: ../../../README.rst:141 c60bd5881b384a78804e3902615df2b0
msgid "puorth: point of spelling uniqueness"
msgstr "puorth : point d'unicité orthographique"

#: ../../../README.rst:142 2270a168b07b4942b722aa75fc421033
msgid "puphon: point of phonological uniqueness"
msgstr "puphon : point d'unicité phonologique"

#: ../../../README.rst:143 a07b37ec7b6d4b73a3928e833c61643e
msgid "syll: syllable phonological form"
msgstr "syllabe : forme phonologique des syllabes"

#: ../../../README.rst:144 8f9472d7f64e4794a4bea518eaf5f7bf
msgid "nbsyll: number of syllables"
msgstr "nbsyll : nombre de syllabes"

#: ../../../README.rst:145 53797f1433184a2987c0ac28abdb7e1f
msgid "cv-cv: syllable phonological structure"
msgstr "cv-cv : structure phonologique des syllabes"

#: ../../../README.rst:146 95da0199c8164940a687eda02adfe6e9
msgid "orthrenv: reverse orthographic form"
msgstr "orthrenv : forme orthographique inversée"

#: ../../../README.rst:147 c850533290a84cb2af50a2439a481d13
msgid "phonrenv: reversed phonological form"
msgstr "phonrenv : forme phonologique inversée"

#: ../../../README.rst:148 88bfbcb27c554e7b9c179f3c0e162931
msgid "orthosyll: syllable orthographic form"
msgstr "orthosyll : forme orthographique des syllabes"

#: ../../../README.rst:149 f7cb2b2d9bcb49d98dfc83a8cab97386
msgid ""
"cgramortho: the different grammatical category for a given orthographic "
"representation"
msgstr ""
"cgramortho : les différentes catégories grammaticales pour une "
"représentation orthographique donnée"

#: ../../../README.rst:150 bfdc6fc720674edab4fa4d07f74ab356
msgid ""
"deflem: the percentage of people who said they knew the lemma of the word"
msgstr ""
"deflem : le pourcentage de personnes qui ont déclaré connaître le lemme du "
"mot."

#: ../../../README.rst:151 2b428ce0dcbe4023b2e60677adfe85a4
msgid "defobs: the size of the sample from which 'deflem' is derived"
msgstr "defobs : la taille de l'échantillon d'où est tiré 'deflem'."

#: ../../../README.rst:152 90df4a93cae0415faf50579ebb378b80
msgid "old20:  orthographic Levenshtein Distance"
msgstr "old20 : distance de Levenshtein orthographique"

#: ../../../README.rst:153 27824bd2b1e14283ad3a1fd977776e00
msgid "pld20: phonological Levenshtein Distance"
msgstr "pld20 : distance de Levenshtein phonologique"

#: ../../../README.rst:154 34f475d0e2ee44dcb37cb42aab3dd740
msgid "morphoder: inflectional morphology"
msgstr "morphoder : morphologie flexionnelle"

#: ../../../README.rst:155 fbf8e0a2d94440bcb04d58d64b77d06b
msgid "nbmorph: the number of morphemes directly computed from 'morphoder'"
msgstr ""
"nbmorph : le nombre de morphèmes directement calculé à partir de "
"'morphoder'."

